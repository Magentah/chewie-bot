{
  "name": "chewie-bot",
  "version": "0.0.1",
  "main": "main.js",
  "author": "Magentah <Magentah@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "setup": "yarn install && cd src/client && yarn install",
    "start-dev:build": "cd src/client && yarn build && cd ../.. && nodemon --config \"./util/nodemon.json\"/ src/start.ts",
    "start-dev": "nodemon --config \".util/nodemon.json\"/ src/start.ts",
    "start-debug:build": "cd src/client && yarn build && cd ../.. && nodemon --config \"./util/nodemon.json\"/ --inspect src/start.ts",
    "start-debug": "nodemon --config \"./util/nodemon.json\"/ --inspect src/start.ts",
    "tsc": "tsc",
    "start-static": "cd src/client && yarn build && cd .. && copyfiles \"client/build/**/*\" ../dist/ && cd .. && yarn tsc && node ./dist/start.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/inversify-express-utils": "^2.0.0",
    "@types/request": "^2.48.1",
    "@types/request-promise-native": "^1.0.16",
    "@types/sqlite3": "^3.1.5",
    "@types/tmi.js": "^1.4.0",
    "copyfiles": "^2.1.1",
    "inversify": "^5.0.1",
    "knex": "^0.19.5",
    "nodemon": "^1.19.1",
    "reflect-metadata": "^0.1.13",
    "request-promise-native": "^1.0.7",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@overnightjs/core": "^1.6.9",
    "@overnightjs/logger": "^1.1.9",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/node-cache": "^4.1.3",
    "express": "^4.17.1",
    "http-status-codes": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.6.0",
    "minimist-string": "^1.0.2",
    "moment": "^2.24.0",
    "node-cache": "^4.2.1",
    "request": "^2.88.0",
    "sqlite3": "^4.0.9",
    "tmi.js": "^1.4.5",
    "winston": "^3.2.1"
  }
}
