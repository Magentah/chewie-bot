{
    "name": "chewie-bot",
    "version": "0.0.1",
    "main": "main.js",
    "author": "Magentah <Magentah@users.noreply.github.com>",
    "license": "MIT",
    "scripts": {
        "setup": "yarn install && cd ../client && yarn install",
        "setup-production": "yarn install --prod && cd ../client && yarn install --prod",
        "start-dev:build": "cd ../client && yarn build && cd ../server && nodemon --config \"./util/nodemon.json\"/ src/start.ts",
        "start-dev": "nodemon --config \".util/nodemon.json\"/ src/start.ts",
        "start-debug:build": "cd ../client && yarn build && cd ../server && nodemon --config \"./util/nodemon.json\"/ --inspect src/start.ts",
        "start-debug": "nodemon -L -e ts --config \"./util/nodemon.json\"/ --inspect=0.0.0.0:9229 src/start.ts",
        "tsc": "tsc",
        "start-static": "cd ../client && yarn build && cd ../server && copyfiles \"../client/build/**/*\" ./dist/ && cd .. && yarn tsc && node ./dist/start.js",
        "build": "cd ../server && yarn tsc && cd ../client && yarn build",
        "start": "cd ../server/dist && node start.js"
    },
    "devDependencies": {
        "@types/express-session": "^1.17.4",
        "copyfiles": "^2.4.1",
        "nodemon": "^2.0.15",
        "reflect-metadata": "^0.1.13",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.12.0",
        "tslint": "^6.1.3",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "@overnightjs/core": "^1.7.6",
        "@overnightjs/logger": "^1.2.1",
        "@types/connect-redis": "0.0.16",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/inversify-express-utils": "^2.0.0",
        "@types/node-cache": "^4.1.3",
        "@types/node-cron": "^2.0.5",
        "@types/passport": "^1.0.7",
        "@types/passport-oauth2": "^1.4.11",
        "@types/redis": "^2.8.32",
        "@types/socket.io-client": "^1.4.36",
        "@types/sqlite3": "^3.1.8",
        "@types/tmi.js": "^1.8.1",
        "@types/ws": "^7.4.7",
        "axios": "^0.21.4",
        "busboy-body-parser": "^0.3.2",
        "connect-redis": "^5.0.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "debug": "^4.3.3",
        "express": "^4.17.2",
        "express-session": "^1.17.1",
        "guid-typescript": "^1.0.9",
        "http-status-codes": "^2.2.0",
        "inversify": "^5.0.5",
        "knex": "^2.0.0",
        "link-preview-js": "^2.1.10",
        "moment": "^2.29.1",
        "node-cache": "^5.1.2",
        "node-cron": "^3.0.0",
        "passport": "^0.4.1",
        "passport-oauth2": "^1.6.1",
        "redis": "^3.0.2",
        "socket.io-client": "^2.4.0",
        "sqlite3": "5.0.2",
        "tmi.js": "^1.8.5",
        "tslib": "^2.3.1",
        "winston": "^3.4.0",
        "winston-daily-rotate-file": "^4.6.0",
        "ws": "^7.5.6"
    }
}
