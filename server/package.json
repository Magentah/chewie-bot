{
    "name": "chewie-bot",
    "version": "0.0.1",
    "main": "main.js",
    "author": "Magentah <Magentah@users.noreply.github.com>",
    "license": "MIT",
    "scripts": {
        "setup": "yarn install && cd ../client && yarn install",
        "setup-production": "yarn install --prod && cd ../client && yarn install --prod",
        "start-dev:build": "cd ../client && yarn build && cd ../server && nodemon --config \"./util/nodemon.json\"/ src/start.ts",
        "start-dev": "nodemon --config \".util/nodemon.json\"/ src/start.ts",
        "start-debug:build": "cd ../client && yarn build && cd ../server && nodemon --config \"./util/nodemon.json\"/ --inspect src/start.ts",
        "start-debug": "nodemon --config \"./util/nodemon.json\"/ --inspect src/start.ts",
        "tsc": "tsc",
        "start-static": "cd ../client && yarn build && cd ../server && copyfiles \"../client/build/**/*\" ./dist/ && cd .. && yarn tsc && node ./dist/start.js",
        "build": "cd ../server && yarn tsc && cd ../client && yarn build",
        "start": "cd ../server/dist && node start.js"
    },
    "devDependencies": {
        "@types/express-session": "^1.17.3",
        "copyfiles": "^2.4.1",
        "nodemon": "^2.0.7",
        "reflect-metadata": "^0.1.13",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0",
        "tslint": "^6.1.3",
        "typescript": "^4.2.3"
    },
    "dependencies": {
        "@overnightjs/core": "^1.6.9",
        "@overnightjs/logger": "^1.1.9",
        "@types/connect-redis": "0.0.16",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.10",
        "@types/express": "^4.17.11",
        "@types/inversify-express-utils": "^2.0.0",
        "@types/jsonwebtoken": "^8.5.1",
        "@types/node-cache": "^4.1.3",
        "@types/passport": "^1.0.6",
        "@types/passport-oauth2": "^1.4.10",
        "@types/redis": "^2.8.28",
        "@types/socket.io-client": "^1.4.36",
        "@types/sqlite3": "^3.1.7",
        "@types/tmi.js": "^1.7.1",
        "@types/ws": "^7.4.0",
        "axios": "^0.21.1",
        "connect-redis": "^5.0.0",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "debug": "^4.3.1",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "http-status-codes": "^2.1.4",
        "inversify": "^5.0.5",
        "jsonwebtoken": "^8.5.1",
        "jwks-rsa": "^2.0.1",
        "knex": "^0.95.2",
        "minimist-string": "^1.0.2",
        "moment": "^2.29.1",
        "node-cache": "^5.1.2",
        "passport": "^0.4.1",
        "passport-oauth2": "^1.5.0",
        "redis": "^3.0.2",
        "socket.io-client": "^2.4.0",
        "sqlite3": "^5.0.2",
        "tmi.js": "^1.7.5",
        "tslib": "^2.1.0",
        "winston": "^3.2.1",
        "ws": "^7.4.4"
    }
}
